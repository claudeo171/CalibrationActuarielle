<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:OnlineCalibrator.MAUI"
             xmlns:common="clr-namespace:OnlineCalibrator.SharedPages;assembly=OnlineCalibrator.SharedPages"
             x:Class="OnlineCalibrator.MAUI.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type common:App}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
